                <wf:WebBrowser Name="WebBrowser" />
            </WindowsFormsHost.Child>

        </WindowsFormsHost>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="148" Margin="28,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="211"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="41" Margin="212,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>
